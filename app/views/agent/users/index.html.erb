<div class="container-fluid">
  <h3 class="page-title">用户表</h3>
  <div class="row">
    <div class="col-md-12">
      <!-- BORDERED TABLE -->
      <div class="panel">
        <div class="panel-heading">
        </div>
        <div class="panel-body">
          <table class="table table-bordered" style="margin-bottom: 0">
            <thead>
            <tr>
              <th>#</th>
              <th>账号</th>
              <th>注册时间</th>
              <th>推荐人</th>
              <th>操作</th>
            </tr>
            </thead>
            <tbody>
              <%@users.each do |ele|%>
                <tr>
                  <td><%=ele.id%></td>
                  <td><%=ele.login%></td>
                  <td><%=ele.referee%></td>
                  <td><%=ele.created_at.strftime('%Y-%m-%d %H:%M:%S')%></td>
                  <td>
                    <a href="<%=agent_user_path(ele)%>">用户详情</a>
                  </td>
                </tr>
              <%end%>
            </tbody>
          </table>
          <div class="dataTables_wrapper no-footer" style="text-align: right">
            <%= paginate @users %>
          </div>
        </div>
      </div>
      <!-- END BORDERED TABLE -->
    </div>
  </div>
</div>