<%=form_tag(@param_pay['url'],method: :post,id:"form") do%>
  <%=hidden_field_tag(:MCHNTCD,@param_pay['MCHNTCD'])%>
  <%=hidden_field_tag(:FM,@param_pay['FM'])%>
  <%=hidden_field_tag(:ENCTP,'0')%>
  <%=hidden_field_tag(:VERSION,'2.0')%>

<%end%>

<script type="text/javascript" charset="utf-8">
  document.getElementById('form').submit();
</script>