    <label class="form-control-label">产品: </label>
    <select name="loan[product_id]" class="form-control select2" data-placeholder="Choose Browser">
      <% Product.where({status:true}).each do |ele| %>
          <option value="<%=ele.id%>"<%= @loan.product_id==ele.id ? 'selected' : '' %>><%=ele.name%></option>
      <% end %>
    </select>
  </div>
</div>
