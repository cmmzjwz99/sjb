<div class="sl-pagebody">
  <%= form_for([:admin, @team], :html => {enctype: 'multipart/form-data', class: 'stdform'}) do |f| %>
      <div class="sl-page-title" style="margin-bottom: 0">
        <div class="card pd-20 pd-sm-40">
          <h6 class="card-body-title">团队</h6>

          <div class="form-layout">
            <div class="row ">
              <div class="col-6">
                <div class="form-group">
                  <label class="form-control-label">名称: </label>
                  <%= text_field_tag('team[name]', @team.name, {class: 'form-control'}) %>
                </div>
              </div>

            </div><!-- row -->
            <div class="form-layout-footer">
              <button class="btn btn-info mg-r-5">保存</button>
            </div><!-- form-layout-footer -->
          </div>

        </div>
      </div><!-- sl-page-title -->
