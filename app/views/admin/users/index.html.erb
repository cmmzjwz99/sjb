<nav class="breadcrumb sl-breadcrumb">
  <!--a class="breadcrumb-item" href="index.html">Starlight</a>
  <a class="breadcrumb-item" href="index.html">Pages</a-->
  <span class="breadcrumb-item active">用户管理</span>
</nav>

<div class="sl-pagebody">
  <div class="sl-page-title">
    <div class="card pd-20 pd-sm-40">
      <h6 class="card-body-title">用户列表</h6>
      <div id="datatable1_filter" class="dataTables_filter"><label><input type="search" class="" placeholder="Search..." aria-controls="datatable1"></label></div>
      <table class="table table-hover table-bordered mg-b-0">
        <thead class="bg-info">
        <tr>
          <th>id</th>
          <th>名字</th>
        </tr>
        </thead>
        <tbody>
        <%@users.each do |ele|%>
            <tr>
              <td><%=ele.id%></td>
              <td><a href="<%=admin_user_path(ele)%>"><%=ele.login%></a></td>
            </tr>
        <%end%>
        </tbody>
      </table>
      <div class="dataTables_wrapper no-footer">
        <%= paginate @users %>
      </div>
    </div>
  </div><!-- sl-page-title -->

</div><!-- sl-pagebody -->